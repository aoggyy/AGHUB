---AutoStat---
local Tab = Window:NewTab("Misc")
local Section = Tab:NewSection("AutoStat")
---Melee---
Section:NewToggle("Melee", "AutoStatMelee", function(state)
    if state then
        _G.AutoStat = true
while _G.AutoStat do
local args = {
    [1] = "AddPoint",
    [2] = "Melee",
    [3] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
wait()
end
    else
       _G.AutoStat = false
while _G.AutoStat do
local args = {
    [1] = "AddPoint",
    [2] = "Melee",
    [3] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
wait()
end
    end
end)
---Defense---
Section:NewToggle("Defense", "AutoStatDefense", function(state)
    if state then
        _G.AUTOSTAT = true
        while _G.AUTOSTAT do
local args = {
    [1] = "AddPoint",
    [2] = "Defense",
    [3] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
wait()

end
    else
    _G.AUTOSTAT = false
        while _G.AUTOSTAT do
local args = {
    [1] = "AddPoint",
    [2] = "Defense",
    [3] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
wait()
        end
end
end)
